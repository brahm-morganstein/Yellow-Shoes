                                        <div class="hero-cta-btn-half">
                                            {%- assign cta_text = block.settings.buttons_right_top_text -%}
                                            {%- assign cta_url = block.settings.buttons_right_top_href -%}
                                            {%- if shop.locale == 'fr' -%}
                                                {%- assign cta_text = block.settings.buttons_right_top_text_fr -%}
                                                {%- assign cta_url = block.settings.buttons_right_top_href_fr -%}
                                            {%- endif -%}
                                            <a href="{{ cta_url }}" class="hero-cta-btn"
                                               id="hero-cta-btn-top-right-{{ section.id }}-{{ forloop.index }}">{{ cta_text }}</a>
                                        </div>
                                        <div class="hero-cta-btn-half">
                                            {%- assign cta_text = block.settings.buttons_left_bottom_text -%}
                                            {%- assign cta_url = block.settings.buttons_left_bottom_href -%}
                                            {%- if shop.locale == 'fr' -%}
                                                {%- assign cta_text = block.settings.buttons_left_bottom_text_fr -%}
                                                {%- assign cta_url = block.settings.buttons_left_bottom_href_fr -%}
                                            {%- endif -%}
                                            <a href="{{ cta_url }}" class="hero-cta-btn"
                                               id="hero-cta-btn-bottom-left-{{ section.id }}-{{ forloop.index }}">{{ cta_text }}</a>
                                        </div>
                                        <div class="hero-cta-btn-half">
                                            {%- assign cta_text = block.settings.buttons_right_bottom_text -%}
                                            {%- assign cta_url = block.settings.buttons_right_bottom_href -%}
                                            {%- if shop.locale == 'fr' -%}
                                                {%- assign cta_text = block.settings.buttons_right_bottom_text_fr -%}
                                                {%- assign cta_url = block.settings.buttons_right_bottom_href_fr -%}
                                            {%- endif -%}
                                            <a href="{{ cta_url }}" class="hero-cta-btn"
                                               id="hero-cta-btn-bottom-right-left-{{ section.id }}-{{ forloop.index }}">{{ cta_text }}</a>
                                        </div>
                                        <!-- Button 5 Text -->
                                        <div class="hero-cta-btn-full">
                                            {%- assign cta_text = block.settings.buttons_bottom_text -%}
                                            {%- assign cta_url = block.settings.buttons_bottom_href -%}
                                            {%- if shop.locale == 'fr' -%}
                                                {%- assign cta_text = block.settings.buttons_bottom_text_fr -%}
                                                {%- assign cta_url = block.settings.buttons_bottom_href_fr -%}
                                            {%- endif -%}
                                            {% assign is_catalogue = true %}
                                            {% if is_catalogue %}
                                                <a href="{{ cta_url }}" class="hero-cta-btn" data-src="{{ cta_url }}" data-fancybox="" data-type="iframe"
                                                   id="hero-cta-btn-bottom-{{ section.id }}-{{ forloop.index }}">{{ cta_text }}</a>
                                            {% else %}
                                                <a href="{{ cta_url }}" class="hero-cta-btn"
                                                   id="hero-cta-btn-bottom-{{ section.id }}-{{ forloop.index }}">{{ cta_text }}</a>
                                            {% endif %}
                                        </div>
                                        {% if block.settings.enable_hero_tooltip %}
                                            <div style="position:relative; display:inline-block;width:100%;">
                                                <div style="position:absolute;left:50%; transform:translateX(-50%);bottom: -25px;">
                                                    <i id="announcement-tooltip-btn-2--{{ section.id }}"
                                                       class="fas fa-info-circle tooltip__btn tooltip__btn--2"
                                                       style="cursor:pointer;color: {{ block.settings.toolip_icon_color }}; position: unset; vertical-align:middle;"></i>
                                                    {% if shop.locale == 'en' %}
                                                        <span style="font-size:0.9em; vertical-align:middle; color: {{ block.settings.tooltip_subheading_color }};">{{ block.settings.tooltip_subheading }}</span>
