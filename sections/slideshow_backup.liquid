                        {% if block.settings.enable_hero_tooltip %}
                            <script>
                              $(document).ready(function() {
                                let $body = $('body');

                                $('#announcement-tooltip-btn-2--{{ section.id }}').on('mouseover', function(evt) {
                                  evt.preventDefault();
                                  $('#announcement-tooltip-text-2--{{ section.id }}').fadeIn('slow');
                                });

                                $('.close-tooltip-2').on('click', function() {

                                  $('#announcement-tooltip-text-2--{{ section.id }}').fadeOut('slow');
                                });

                                $('#announcement-tooltip-btn-3--{{ section.id }}').on('click', function(evt) {
                                  evt.preventDefault();
                                  $('#announcement-tooltip-text-3--{{ section.id }}').fadeIn('slow');
                                });

                                $('.close-tooltip-3').on('click', function() {
                                  $('#announcement-tooltip-text-3--{{ section.id }}').fadeOut('slow');
                                });
