                                            {%- if shop.locale == 'fr' -%}
                                                {%- assign cta_text = block.settings.buttons_left_top_text_fr -%}
                                                {%- assign cta_url = block.settings.buttons_left_top_href_fr -%}
                                            {%- endif -%}
                                            <a href="{{ cta_url }}" class="hero-cta-btn"
                                               id="hero-cta-btn-top-left-{{ section.id }}-{{ forloop.index }}">{{ cta_text }}</a>
