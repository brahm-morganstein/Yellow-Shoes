<script>
  $(document).ready(function(){
    $('#selectShoeSize').select2({
      placeholder: window.location.toString().includes('/fr') ? 'Sélectionner' : 'Select'
    });
    $('#selectAudience').select2();
    //Get Provinces
    $.getJSON('https://app.yellowshoes.com/api/provinces',function(provinces){
      //console.log(provinces);
      provinces.forEach(function(province){
          var option = $('<option>');
          if(language=='en'){
            option.html(province.name_en);
          }else{
            option.html(province.name_fr);
          }
          option.val(province.name_en);
          $("#selectProvince").append(option);
      });
      $("#selectProvince").prop('disabled', false);    
    }); 
    //
    $("#selectAudience").change(function(){
      $("#other").hide()
      if($("#selectAudience").val() == 'other'){
        $("#other").show()
      }
    });

    $("#has_prev_exp_yes").change(function(){
      if($(this).is(":checked")){
        $("#prev_biz_container").show();
      }else{
         $("#prev_biz_container").hide();
      }
    });
    $("#has_prev_exp_no").change(function(){
      if($(this).is(":checked")){
        $("#prev_biz_container").hide();
      }else{
         $("#prev_biz_container").show();
      }
    });    
    //
    $("#fileMedia").change(function(){
      $(".btn-upload").html(this.files[0].name);
    });

    $("#selectShoeSize").change(function(){
      var values = $(this).val()
      $("#shoe_size_container").html("")
      values.forEach(function(value,index){
        var html = getOptions(value)
        $("#shoe_size_container").append(html)
        $(`#${value}_select`).select2()
      })
    })

    function getOptions(value){
      switch(value){
        case 'women':
          return `
            <div class="mt-10">
              <label>{% if shop.locale == "en" %} Women shoe sizes {% else %} Pointures pour femmes {% endif %}</label>
              <select id="${value}_select" class="m-select input-full" multiple="multiple">
                <option value="5">5</option>
                <option value="6">6</option>
                <option value="7">7</option>
                <option value="8">8</option>
                <option value="9">9</option>
                <option value="10">10</option>      
                <option value="11">11</option> 
              </select>
            </div> 
          `
          break          
        case 'men':
          return `
            <div class="mt-10">
              <label>{% if shop.locale == "en" %} Men shoe sizes {% else %} Pointures pour hommes {% endif %}</label>
              <select id="${value}_select" class="m-select input-full" multiple="multiple">
                <option value="7">7</option>
                <option value="8">8</option>
                <option value="9">9</option>
                <option value="10">10</option>
                <option value="11">11</option>
                <option value="12">12</option>    
                <option value="13">13</option>    
              </select>
            </div>
          `
          break
        case 'babies':
          return `
            <div class="mt-10">
              <label>{% if shop.locale == "en" %} Babies shoe sizes {% else %} Pointures pour bébés {% endif %}</label>
              <select id="${value}_select" class="m-select input-full" multiple="multiple">
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
                <option value="6">6</option>
                <option value="7">7</option>
                <option value="8">8</option>    
                <option value="9">9</option>    
              </select>
            </div>
          `
          break   
        case 'toddlers':
          return `
            <div class="mt-10">
              <label>{% if shop.locale == "en" %} Toddler(s) shoe sizes {% else %} Pointures pour tout(es)-petit(e)s {% endif %}</label>
              <select id="${value}_select" class="m-select input-full" multiple="multiple">
                <option value="10">10</option>
                <option value="11">11</option>
                <option value="12">12</option>
                <option value="13">13</option>   
              </select>
            </div>
          `
          break
        case 'juniors':
          return `
            <div class="mt-10">
              <label>{% if shop.locale == "en" %} Junior shoe sizes {% else %} Pointures pour juniors {% endif %}</label>
              <select id="${value}_select" class="m-select input-full" multiple="multiple">
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>  
                <option value="5">5</option>
                <option value="6">6</option>
                <option value="7">7</option>
                <option value="8">8</option>                  
              </select>
            </div>
          `
          break          
      }
    }

    $("#btnSubmit").click(function(){
      $(".error_label").hide();
      $("#btnSubmit").prop('disabled', true);

      var shoe_sizes = $("#selectShoeSize").val();

      var fd = new FormData();
      //
      fd.append('first_name',$("#inputFirstName").val());
      fd.append('last_name',$("#inputLastName").val());
      fd.append('email',$("#inputEmail").val());
      fd.append('phone_number',$("#inputPhoneNumber").val());
      fd.append('street_address',$("#inputStreetAddress").val());
      fd.append('city',$("#inputCity").val());
      fd.append('province',$("#selectProvince").val());
      fd.append('zip',$("#inputZip").val());
      fd.append('language',$("#selectLanguage").val());
      fd.append('shoe_size',shoe_sizes);
      fd.append('audience',$("#selectAudience").val());
      fd.append('motivation',$("#motivation").val());
      fd.append('goals',$("#goals").val());

      if($('#has_prev_exp_yes').is(":checked")){
        fd.append('prev_biz',$("#prev_biz").val());
        fd.append('prev_biz_description',$("#prev_biz_description").val());
      }

      if(shoe_sizes.length > 0){
        shoe_sizes.forEach(function(value,index){
          fd.append(`${value}_sizes`,$(`#${value}_select`).val());
        })
      }

      var social_media = [];
      
      if($("#instagram_profile").val().trim() != ''){
        social_media.push({
          platform:'instagram',
          profile:$("#instagram_profile").val(),
          subscribers:$("#instagram_subscribers").val(),
          engagement:$("#instagram_engagement").val()
        })
      }

      if($("#tiktok_profile").val().trim() != ''){
        social_media.push({
          platform:'tiktok',
          profile:$("#tiktok_profile").val(),
          subscribers:$("#tiktok_subscribers").val(),
          engagement:$("#tiktok_engagement").val()
        })
      }  

      if($("#youtube_profile").val().trim() != ''){
        social_media.push({
          platform:'youtube',
          profile:$("#youtube_profile").val(),
          subscribers:$("#youtube_subscribers").val(),
          engagement:$("#youtube_engagement").val()
        })
      }  

      if($("#x_profile").val().trim() != ''){
        social_media.push({
          platform:'x',
          profile:$("#x_profile").val(),
          subscribers:$("#x_subscribers").val(),
          engagement:$("#x_engagement").val()
        })
      } 

      if($("#facebook_profile").val().trim() != ''){
        social_media.push({
          platform:'facebook',
          profile:$("#facebook_profile").val(),
          subscribers:$("#facebook_subscribers").val(),
          engagement:$("#facebook_engagement").val()
        })
      }       

      fd.append('social_medias',JSON.stringify(social_media));

      // Check file selected or not
      var files = $('#fileMedia')[0].files;
      if(files.length > 0 ){
        fd.append('media',files[0]);
      }
      //
      fd.append('g-recaptcha-response',grecaptcha.getResponse());
      //
      fd.append('terms',$("#chkboxTerms").is(':checked'));
      
      //
      $.ajax({
        url: 'https://app.yellowshoes.com/api/partnership-program',
        type: 'post',
        data: fd,
        contentType: false,
        processData: false,
        beforeSend: function(request) {
          //CORS
          request.setRequestHeader("X-Requested-With", "XMLHttpRequest");
        },        
        success: function(response){
          $("#partnership-header").hide()
          $("#partnership-form").hide()
          $("#partnership-succes-message").show();
         Swal.fire({
           icon: "success"
         });
          //window.location.hash = '#partnership-succes-message';
          //grecaptcha.reset();
          //$("#btnSubmit").prop('disabled', false);                       
        },
        error:function(data){
          console.log(data.status);
          grecaptcha.reset();
          //handle form errors
          if(data.status === 422){
            let response = $.parseJSON(data.responseText);
            $.each(response.errors,function(key,value){
              console.log(key+":"+value);
              $("#error_"+key).show();
            });
          }
          if(data.status === 413 || data.status === 0){
              $("#error_media").show();
          }
          $("#btnSubmit").prop('disabled', false);
        }
      });
    });
  })
</script>