    "rich_text_9HbeM4": {
      "type": "rich-text",
      "blocks": {
        "heading_J8DyeK": {
          "type": "heading",
          "disabled": true,
          "settings": {
            "title": "Riverland Heritage Collection",
            "title_fr": "LA COLLECTION RIVERLAND HÉRITAGE",
